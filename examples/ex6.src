// if statement not returning value, with block sub-expr

f(x : int) -> int = {
  if x = 0 then {
    printf("ello") // could have semicolon but doesn't need.
    // will be used as value of block.
    // but if is statement here, so its value doesn't matter.
  }
  return x;
}

printf("%d\n", f(0));
printf("%d\n", f(1));
printf("%d\n", f(2));